{% extends 'base.html.twig' %}
{% block title %}<title>Armenia Tree Project - News</title>{% endblock %}
{% block body %}
    <hr>
    <table>
        {% for object in pager.currentPageResults %}
            <tr>
                <td>{{ object.Title }}</td>
                <td>{{ object.Text }}</td>
            </tr>
        {% endfor %}
    </table>

    {% if pager.haveToPaginate %}
        {{ pagerfanta(pager) }}
    {% endif %}
    <hr>
{% endblock %}